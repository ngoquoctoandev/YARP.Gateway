{
  "ReverseProxy": {
    "Routes": {
      "OccultismApiRoute": {
        "ClusterId": "OccultismApi",
        "RateLimiterPolicy": "Sliding",
        "CorsPolicy": "CorsPolicy",
        "Match": {
          "Path": "numerology/{**catch-all}",
          "Hosts": [ "localhost", "giaimathanso.com" ],
          "Methods": [ "GET", "POST", "PUT" ]
        },
        "Transforms": [
          {
            "ResponseHeaderRemove": "Server",
            "When": "Always"
          },
          {
            "ResponseHeaderRemove": "X-Powered-By",
            "When": "Always"
          }
        ]
      }
    },
    "Clusters": {
      "OccultismApi": {
        "LoadBalancingPolicy": "PowerOfTwoChoices",
        "Destinations": {
          "http/apidestination": {
            "Address": "http://localhost:5000/api/v1",
            "Health": "http://localhost:5000/"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": "true",
            "Interval": "00:00:10",
            "Timeout": "00:00:10",
            "Policy": "ConsecutiveFailures",
            "Path": "/api/health"
          }
        },
        "Metadata": {
          "ConsecutiveFailuresHealthPolicy.Threshold": "3"
        }
      }
    }
  }
}
